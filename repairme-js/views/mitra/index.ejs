<%- include('templates/footer') -%>
  <script>
    var vm = new Vue({

      el: "#app",
      data: {
        sidebar_menu:{
          beranda: 'active'
        },
        mitra: null,
        myfile: null
      },
      mounted(){
        axios({
          method: 'get',
          url: 'api/mitra/' + '<%= email %>'
        }).then(response => {
          this.mitra = response.data;
        })
      }
      
    })
  </script>