<%- include('templates/header') -%>
<%- include('templates/footer') -%>
<script>
    var vm = new Vue({

        el: "#app",
        data: {
            pelanggan: null
        },
        mounted() {
            axios({
                method: 'get',
                url: '/api/pelanggan/' + '<%= email %>'
            }).then(response => {
                this.pelanggan = response.data;
            })
        }

    })
</script>